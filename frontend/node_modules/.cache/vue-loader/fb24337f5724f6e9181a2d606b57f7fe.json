      Cart (<span>{{ items.length }}</span> items)\r\n            </h5>\r\n\r\n            <div class=\"row mb-4\" v-for=\"(item, i) in items\" :key=\"i\">\r\n              <div class=\"col-md-5 col-lg-3 col-xl-3\">\r\n                <img\r\n                  class=\"img-fluid w-100\"\r\n                  src=\"@/assets/cardImg.jpg\"\r\n                  alt=\"Sample\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"col-md-7 col-lg-9 col-xl-9\">\r\n                <div>\r\n                  <div class=\"d-flex justify-content-between\">\r\n                    <div>\r\n                      <h5>{{ item.title }}</h5>\r\n                      <p class=\"mb-3 text-muted text-uppercase small\">\r\n                        {{ item.name }}\r\n                      </p>\r\n                      <p class=\"mb-2 text-muted text-uppercase small\">\r\n                        {{ item.itemId }}\r\n                      </p>\r\n                    </div>\r\n\r\n                    <div>\r\n                      <div>\r\n                        <div class=\"input-group\">\r\n                          <button\r\n                            href=\"#\"\r\n                            class=\"btn btn-primary\"\r\n                            type=\"button\"\r\n                            id=\"inputGroupFileAddon04\"\r\n                            v-on:click=\"decreaseItemQuantity(item.itemId)\"\r\n                          >\r\n                            -\r\n                          </button>\r\n                          <input\r\n                            type=\"text\"\r\n                            class=\"number\"\r\n                            min=\"0\"\r\n                            v-model=\"item.quantity\"\r\n                            style=\"text-align: center\"\r\n                          />\r\n                          <button\r\n                            href=\"#\"\r\n                            class=\"btn btn-primary\"\r\n                            type=\"button\"\r\n                            id=\"inputGroupFileAddon04\"\r\n                            v-on:click=\"increaseItemQuantity(item.itemId)\"\r\n                          >\r\n                            +\r\n                          </button>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div\r\n                    class=\"d-flex justify-content-between align-items-center\"\r\n                  >\r\n                    <div>\r\n                      <a\r\n                        href=\"#!\"\r\n                        type=\"button\"\r\n                        class=\"card-link-secondary small text-uppercase mr-3\"\r\n                        ><i class=\"fas fa-trash-alt mr-1\"></i> Remove item\r\n                      </a>\r\n                    </div>\r\n                    <p class=\"mb-0\">\r\n                      <span\r\n                        ><strong\r\n                          >€{{ item.totalPrice.toFixed(2) }}</strong\r\n                        ></span\r\n                      >\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- Card -->\r\n      </div>\r\n      <!--Grid column-->\r\n\r\n      <!--Grid column-->\r\n      <div class=\"col-lg-4\" style=\"margin-top: 100px\">\r\n        <!-- Card -->\r\n        <div class=\"card bg-dark border-primary mb-3\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"text-white mb-3\">Shopping cost</h5>\r\n\r\n            <ul class=\"list-group list-group-flush\">\r\n              <li\r\n                class=\"list-group-item bg-dark text-white d-flex justify-content-between align-items-center border-0 px-0 pb-0\"\r\n              >\r\n                Product cost\r\n                <span>€{{ cartPriceValue() }}</span>\r\n              </li>\r\n              <li\r\n                class=\"list-group-item bg-dark text-white d-flex justify-content-between align-items-center px-0\"\r\n              >\r\n                Shipping\r\n                <span>Free</span>\r\n              </li>\r\n              <li\r\n                class=\"list-group-item bg-dark text-white d-flex justify-content-between align-items-center border-0 px-0 mb-3\"\r\n              >\r\n                <div>\r\n                  <strong>Total amount</strong>\r\n                  <strong>\r\n                    <p class=\"mb-0\">(incl. VAT)</p>\r\n                  </strong>\r\n                </div>\r\n                <span\r\n                  ><strong>€{{ cartPriceValue() }}</strong></span\r\n                >\r\n              </li>\r\n            </ul>\r\n\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-primary btn-block waves-effect waves-light\"\r\n            >\r\n              Order\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <!-- Card -->\r\n      </div>\r\n      <!--Grid column-->\r\n    </div>\r\n    <!--Grid row-->\r\n  </section>\r\n  <!--Section: Block Content-->\r\n</template>\r\n\r\n\r\n<script>\r\nimport CartService from \"@/services/CartService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      items: [\r\n        /*  \r\n        {title:'Lenovo', description:'Page 1', price:'100'},\r\n        {title:'Samsung', description:'Page 1', price:'100'},\r\n        {title:'Nokia', description:'Page 1', price:'100'},\r\n        {title:'Apple', description:'Page 1', price:'100'},\r\n        {title:'Huawei', description:'Page 1', price:'100'},\r\n        {title:'Xiaomi', description:'Page 1.', price:'100'},\r\n        {title:'Accer', description:'Page 1.', price:'100'},\r\n        {title:'Assus', description:'Page 1.', price:'100'}\r\n      */\r\n      ],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getItems() {\r\n      CartService.getCartItems()\r\n        .then((response) => (this.items = response.data))\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    increaseItemQuantity(id) {\r\n      CartService.addCartItem(id)\r\n        .then((response) => console.log(response))\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.getItems();\r\n      location.reload();\r\n    },\r\n\r\n    decreaseItemQuantity(id) {\r\n      CartService.removeCartItem(id)\r\n        .then((response) => console.log(response))\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.getItems();\r\n      location.reload();\r\n    },\r\n\r\n    cartPriceValue() {\r\n      var totalCartPrice = 0;\r\n      var item;\r\n\r\n      for (item of this.items) {\r\n        totalCartPrice += item.totalPrice;\r\n      }\r\n\r\n      return totalCartPrice.toFixed(2);\r\n    },\r\n  },\r\n\r\n  beforeMount() {\r\n    this.getItems();\r\n  },\r\n};\r\n</script>\r\n\r\n"]}]}