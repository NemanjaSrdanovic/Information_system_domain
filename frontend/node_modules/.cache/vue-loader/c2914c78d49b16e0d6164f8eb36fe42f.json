rimary mb-3\">\r\n          <div class=\"card-body\">\r\n            <nav id=\"sidebar\">\r\n              <div style=\"color: white; text-align: center\">\r\n                <h3>Dashboard</h3>\r\n              </div>\r\n              <ul style=\"color: white\">\r\n                <li class=\"active\">\r\n                  <a\r\n                    href=\"#homeSubmenu\"\r\n                    data-toggle=\"collapse\"\r\n                    aria-expanded=\"false\"\r\n                    class=\"dropdown-toggle\"\r\n                    v-on:click=\"\r\n                      showTable(0);\r\n                      resetVariables();\r\n                      closeDropdown();\r\n                    \"\r\n                    >Reports\r\n                  </a>\r\n                  <ul class=\"collapse list-unstyled\" id=\"homeSubmenu\">\r\n                    <li>\r\n                      <a\r\n                        href=\"#/account\"\r\n                        v-on:click=\"\r\n                          showTable(1);\r\n                          resetVariables();\r\n                        \"\r\n                        >Category sales report\r\n                      </a>\r\n                    </li>\r\n                    <li>\r\n                      <a\r\n                        href=\"#/account\"\r\n                        v-on:click=\"\r\n                          showTable(2);\r\n                          resetVariables();\r\n                        \"\r\n                        >Product sales report</a\r\n                      >\r\n                    </li>\r\n                  </ul>\r\n                </li>\r\n                <li class=\"active\">\r\n                  <a\r\n                    href=\"#pageSubmenu\"\r\n                    data-toggle=\"collapse\"\r\n                    aria-expanded=\"false\"\r\n                    class=\"dropdown-toggle\"\r\n                    >Products</a\r\n                  >\r\n                  <ul class=\"collapse list-unstyled\" id=\"pageSubmenu\">\r\n                    <li><a href=\"#\">Add product</a></li>\r\n                    <li><a href=\"#\">Edit product</a></li>\r\n                    <li><a href=\"#\">Remove product</a></li>\r\n                  </ul>\r\n                </li>\r\n                <li><a href=\"#\">Charts</a></li>\r\n                <li><a href=\"#\">Profile</a></li>\r\n                <li><a href=\"#\">Support</a></li>\r\n              </ul>\r\n            </nav>\r\n          </div>\r\n        </div>\r\n        <!-- Card -->\r\n      </div>\r\n      <!--Grid column-->\r\n\r\n      <!--Grid column-->\r\n      <div\r\n        class=\"col-lg-8\"\r\n        style=\"margin-top: 100px; margin-bottom: 450px\"\r\n        v-if=\"showCategoryTable || showProductTable\"\r\n      >\r\n        <!-- Card -->\r\n        <div class=\"card bg-dark border-primary mb-3\">\r\n          <div class=\"card-body\">\r\n            <div style=\"text-align: center; margin-bottom: 20px\">\r\n              <date-picker\r\n                style=\"text: center\"\r\n                v-model=\"range\"\r\n                lang=\"en\"\r\n                range\r\n                confirm\r\n                type=\"date\"\r\n                value-type=\"format\"\r\n                format=\"YYYY-MM-DD\"\r\n                placeholder=\"Chose the report range\"\r\n              ></date-picker>\r\n\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-primary active btn-sm\"\r\n                style=\"margin-left: 10px\"\r\n                v-on:click=\"getCategoryData(range)\"\r\n              >\r\n                Load\r\n              </button>\r\n            </div>\r\n\r\n            <table\r\n              class=\"table table-hover table-dark\"\r\n              v-if=\"showCategoryTable\"\r\n            >\r\n              <thead class=\"thead-primary\">\r\n                <tr>\r\n                  <th scope=\"col\">#</th>\r\n                  <th scope=\"col\">Category</th>\r\n                  <th scope=\"col\">Item name</th>\r\n                  <th scope=\"col\">Items sold</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr\r\n                  v-for=\"(product, index) in categoryReportTable\"\r\n                  :key=\"index\"\r\n                >\r\n                  <th scope=\"row\">{{ index }}</th>\r\n                  <td>{{ product.title }}</td>\r\n                  <td>{{ product.description }}</td>\r\n                  <td>1</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n\r\n            <table class=\"table table-hover table-dark\" v-if=\"showProductTable\">\r\n              <thead class=\"thead-primary\">\r\n                <tr>\r\n                  <th scope=\"col\">#</th>\r\n                  <th scope=\"col\">User name</th>\r\n                  <th scope=\"col\">Item name</th>\r\n                  <th scope=\"col\">Items sold</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(product, index) in productReportTable\" :key=\"index\">\r\n                  <th scope=\"row\">{{ index }}</th>\r\n                  <td>{{ product.title }}</td>\r\n                  <td>{{ product.description }}</td>\r\n                  <td>1</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n        <!-- Card -->\r\n      </div>\r\n      <!--Grid column-->\r\n    </div>\r\n    <!--Grid row-->\r\n  </section>\r\n  <!--Section: Block Content-->\r\n\r\n  <div v-else><SignInSignUp /></div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport SignInSignUp from \"@/components/SignIn_SignUp.vue\";\r\nimport AuthService from \"@/services/AuthService\";\r\nimport DatePicker from \"vue2-datepicker\";\r\nimport \"vue2-datepicker/index.css\";\r\n\r\nexport default {\r\n  components: {\r\n    SignInSignUp,\r\n    DatePicker\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      products: [\r\n   \r\n\r\n        { title: \"Lenovo\", description: \"Page 1\" },\r\n        { title: \"Samsung\", description: \"Page 1\" },\r\n        { title: \"Nokia\", description: \"Page 1\" },\r\n        { title: \"Apple\", description: \"Page 1\" },\r\n        { title: \"Huawei\", description: \"Page 1\" },\r\n        { title: \"Xiaomi\", description: \"Page 1.\" },\r\n        { title: \"Accer\", description: \"Page 1.\" },\r\n        { title: \"Assus\", description: \"Page 1.\" },\r\n        { title: \"Dell\", description: \"Page 1.\" },\r\n        { title: \"Hama\", description: \"Page 1.\" },\r\n        { title: \"Levis\", description: \"Page 1.\" },\r\n        { title: \"Amazon\", description: \"Page 1.\" },\r\n        { title: \"Lenovo\", description: \"Page 2.\" },\r\n        { title: \"Samsung\", description: \"Page 2.\" },\r\n      ],\r\n\r\n      range: \"\",\r\n      showCategoryTable: false,\r\n      showProductTable: false,\r\n      categoryTableId: 1,\r\n      productTableId: 2,\r\n      categoryReportTable: [],\r\n      productReportTable: [],\r\n      employeeLoggedIn: false\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    showTable(tableId) {\r\n      if (tableId == this.categoryTableId) {\r\n        this.showCategoryTable = true;\r\n        this.showProductTable = false;\r\n      } else if (tableId == this.productTableId) {\r\n        this.showCategoryTable = false;\r\n        this.showProductTable = true;\r\n      } else {\r\n        this.showProductTable = false;\r\n        this.showProductTable = false;\r\n      }\r\n    },\r\n\r\n    getCategoryData(range) {\r\n      console.log(\"RANGE:\");\r\n      console.log(range);\r\n\r\n      //UBACITI AXIOS\r\n\r\n      this.categoryReportTable = this.products.slice(0, 5);\r\n    },\r\n\r\n    resetVariables() {\r\n      this.categoryReportTable = [];\r\n      this.productReportTable = [];\r\n      this.range = \"\";\r\n    },\r\n    closeDropdown() {\r\n      if (this.showCategoryTable == true || this.showProductTable == true) {\r\n        location.reload();\r\n      }\r\n    },\r\n\r\n    isEmployeeLogedIn() {\r\n      AuthService.isEmployeeLogedIn()\r\n        .then((response) => (this.employeeLoggedIn = response.data))\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n    }\r\n  },\r\n    beforeMount() {\r\n    this.isEmployeeLogedIn();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\na,\r\na:hover,\r\na:focus {\r\n  color: inherit;\r\n  text-decoration: none;\r\n  transition: all 0.3s;\r\n}\r\n\r\n#sidebar ul li a {\r\n  padding: 10px;\r\n  font-size: 1.1em;\r\n  display: block;\r\n}\r\n\r\n#sidebar ul li a:hover {\r\n  color: #fff;\r\n  background: #0066ff;\r\n}\r\n\r\na[data-toggle=\"collapse\"] {\r\n  position: relative;\r\n}\r\n\r\n.dropdown-toggle::after {\r\n  display: block;\r\n  position: absolute;\r\n  top: 50%;\r\n  right: 20%;\r\n  transform: translateY(-50%);\r\n}\r\nul ul a {\r\n  font-size: 0.9em !important;\r\n  padding-left: 30px !important;\r\n}\r\n\r\nul li {\r\n  list-style-type: none;\r\n}\r\n\r\n.table-hover tbody tr:hover td,\r\n.table-hover tbody tr:hover th {\r\n  background: #0066ff;\r\n}\r\n</style>"]}]}